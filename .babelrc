{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "extensions": [
        ".native.js",
        ".android.js",
        ".ios.js",
        ".js"
      ],
      "alias": {
        "styled-components": "styled-components/native"
      }
    }]
  ]
}
