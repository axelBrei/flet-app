{
  "name": "fletex_app",
  "nameToShow": "FletApp",
  "version": "0.0.1",
  "testing-version": 14,
  "private": true,
  "proxy": "http://localhost:3000",
  "scripts": {
    "start:web:dev": "webpack serve --progress --env=dev --color --mode development --host 0.0.0.0 --config ./webpack.config.js",
    "start:web:prod": "webpack serve --progress --env=prod --color --mode production --config ./webpack.config.js",
    "bundle_android": "react-native bundle --platform android --dev false --entry-file index.native.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "start:android": "npx react-native run-android",
    "start:ios": "npx react-native run-ios --simulator=\"iPhone 11\"",
    "start": "react-native start",
    "lint": "eslint .",
    "pull-web-enviorment": "chmod +x ./pullEnviorment.sh && ./pullEnviorment.sh",
    "build:web:prod": "webpack --mode production --env=prod",
    "cleanAndroid": "cd android && rm -rf app/build && ./gradlew clean && cd ..",
    "web": "webpack-dev-server -d --config ./webpack.config.js --inline --hot --colors",
    "postinstall": "node ./node_modules/@darron1217/react-native-background-geolocation/scripts/postlink.js",
    "bundle-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/resto_mobile/main.jsbundle --assets-dest ios",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "assembleAndroid": "./assembleNativeApp.sh android",
    "assembleAndExportIos": "./assembleNativeApp.sh ios"
  },
  "dependencies": {
    "@darron1217/react-native-background-geolocation": "^0.6.6",
